{
  "name": "Skype English Noting",
  "description": "Skype English Noting using a Skype Bot to send the translated message to a conversation",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "all_frames": false,
      "js": [
        "content/js/inject/noter.js"
      ],
      "matches": [
        "\u003Call_urls\u003E"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab",
    "tabs",
    "storage",
    "notifications"
  ],
  "web_accessible_resources": [
    "content/images/noter.png", "content/images/connect.png", "content/images/connect_slack.png", "content/images/ox.png", "content/images/cam.png",  "content/images/speaker.png", "content/images/slack_member_id.gif", "popup.html"
  ],
  "icons": {
    "128": "content/icon/icon128.png",
    "16": "content/icon/icon16.png",
    "32": "content/icon/icon32.png",
    "48": "content/icon/icon48.png"
  }
}